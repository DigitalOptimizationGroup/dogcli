    "@types/diff": "^4.0.2",
    "@types/eventsource": "^1.1.0",
    "@types/glob": "^7.1.1",
    "@types/inquirer": "^6.0.1",
    "@types/js-yaml": "^3.12.1",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/pako": "^1.0.1",
    "@types/tar-fs": "^1.16.1",
    "chai": "^4",
